<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.kartina.tv"
       name="Kartina.TV"
       version="1.3.6"
       provider-name="Eugene Bond">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="video"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Онлайн-телевидение Kartina.TV</summary>
    <description>Онлайн-телевидение Kartina.TV: более 110 русскоязычных каналов и видеотека от 15 евро в месяц</description>
    <platform>all</platform>
  </extension>
</addon>

