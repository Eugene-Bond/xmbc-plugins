<addon id="repository.bond" name="Eugene Bond XBMC Add-ons" version="1.0.1" provider-name="Eugene Bond">
	<extension point="xbmc.addon.repository" name="Eugene Bond XBMC Add-ons">
		<info compressed="false">https://github.com/Eugene-Bond/xmbc-plugins/raw/master/addons.xml</info>
		<checksum>https://github.com/Eugene-Bond/xmbc-plugins/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/Eugene-Bond/xmbc-plugins/raw/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Add-ons from Bond's Add-on Repository</summary>
